

#include <stdio.h>
#include <string.h>

char	*ft_strstr(char *str, char *to_find);

int		main(void)
{
	char	*str;
	char	*to_find;

	str = "Rato Roeu a Roupa do rei de roma";
	to_find = "Roupa";
	
	printf("Meu Codigo: %s\n", ft_strstr(str, to_find));
	printf("Como deve ser: %s\n", strstr(str, to_find));
	return (0);
}
